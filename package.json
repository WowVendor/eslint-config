{
  "name": "wowvendor-eslint-configs",
  "description": "A set of rules for writing javascript applications",
  "version": "1.0.0",
  "scripts": {
    "test": "run-s test:*",
    "test:code": "jest",
    "test:lint": "lerna exec 'npm run test:lint'",
    "bootstrap": "lerna bootstrap --hoist",
    "publish": "lerna publish from-package",
    "release": "lerna version --conventional-commits",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable",
    "postinstall": "npm run bootstrap && husky install"
  },
  "author": "weyheyhey <iweyheyhey@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/wowvendor/eslint-configs.git"
  },
  "keywords": [
    "style guide",
    "typescript",
    "eslint",
    "config",
    "react",
    "lint"
  ],
  "devDependencies": {
    "@commitlint/cli": "^12.1.1",
    "@types/jest": "^26.0.23",
    "@types/react": "^17.0.4",
    "@wowvendor/commitlint-config": "^1.0.6",
    "cz-customizable": "^6.3.0",
    "eslint": "^7.25.0",
    "glob": "^7.1.6",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "lerna": "^4.0.0",
    "lint-staged": "^10.5.4",
    "npm-run-all": "^4.1.5",
    "pinst": "^2.1.6",
    "prettier": "^2.2.1",
    "react": "^17.0.2",
    "typescript": "^4.2.4"
  }
}
